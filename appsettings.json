{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "*",


  "Environment": {
    "Booyah": {
      "Http": "https://booyah.live/",
      "Website": "booyah",
      "Selector": "div[class='input-box']",
      "ChatContainer": "div[class='message-list']",
      "MessageContainer": "div[class='message']",
      "MessageAuthor": "div > div > span:nth-of-type(1) > span",
      "MessageContent": "div > div > span:nth-of-type(3)",
      "CounterContainer": "span[class='viewer-count'] > span",
      "GameContainer": "div[class='game-name'] > span > a"
    },
    "Facebook": {
      "Http": "https://www.facebook.com/",
      "Website": "facebook",
      "Selector": "div[class='f9o22wc5'] > div",
      "ChatContainer": "div[class='rq0escxv j83agx80 cbu4d94t eg9m0zos fh5enmmv k4urcfbm']",
      "MessageContainer": "div[class='tw6a2znq sj5x9vvc d1544ag0 cxgpxx05']",
      "MessageAuthor": "div[class='btwxx1t3 nc684nl6 bp9cbjyn'] > span > span",
      "MessageContent": "div[class='kvgmc6g5 cxmmr5t8 oygrvhab hcukyx3x c1et5uql'] > div",
      "CounterContainer": "div[class='j83agx80 rgmg9uty pmk7jnqg rnx8an3s fcg2cn6m'] > div:nth-of-type(2) > span:nth-of-type(2)",
      "GameContainer": "div[class='qzhwtbm6 knvmm38d'] > span > h2 > span > strong:nth-child(3) > a"
    },
    "Twitch": {
      "Http": "https://www.twitch.tv/",
      "Website": "twitch",
      "Selector": "span[class='ScAnimatedNumber-sc-acnd2k-0 cIxmzd']",
      "ChatContainer": "div[class='simplebar-content']",
      "MessageContainer": "div[class='chat-line__message']",
      "MessageAuthor": "span[class='chat-author__display-name']",
      "MessageContent": "span[class='text-fragment']",
      "CounterContainer": "span[class='ScAnimatedNumber-sc-acnd2k-0 cIxmzd']",
      "GameContainer": "span[class='CoreText-sc-cpl358-0 deWlGg']"
    },
    "Youtube": {
      "Http": "https://www.youtube.com/watch?app=desktop&",
      "Website": "youtube",
      "Selector": "ytd-live-chat-frame[id='chat']",
      "ChatContainer": "ytd-live-chat-frame[id='chat']",
      "MessageContainer": "yt-live-chat-text-message-renderer",
      "MessageAuthor": "span[id='author-name']",
      "MessageContent": "span[id='message']",
      "CounterContainer": "span[class*='view-count']",
      "GameContainer": "div[id='text-container'] > div[id='title']"
    }
  },

  "LiveElements": {
    "BooyahElements": {
      "CloseChatAnnouncement": ""
    },
    "FacebookElements": {
      "CloseChatAnnouncement": "div[class='f9o22wc5'] > div"
    }
  }
}
